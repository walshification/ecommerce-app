<div class="col-md-9" id="stuff">
  <h1><%= @product.name %> <small>By: <%= @product.vendor.name %></small></h1>

  <img src="<%= @product.photo %>">

  <p><%= @product.description %></p>

  <h2><%= @product.display_price %></h2>
  <p><a href="/products/<%= @product.id %>/edit">Edit</a></p>
  <p><a rel="nofollow" data-method="delete" href="/products/<%= @product.id %>/">Delete</a></p>
</div>
<div class="col-md-3" id="shopping-cart">
  <div class="panel panel-default"> 
    <div class="panel-heading"> 
      <h2 class="panel-title"><span class="glyphicon glyphicon-shopping-cart"></span> Shopping Cart</h2> 
    </div>
    <div class="panel-body">
    </div>
  </div>
</div>
